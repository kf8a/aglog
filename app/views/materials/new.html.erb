<h1>New material</h1>
<% set_material_type_ids %>
<%= render "shared/error_messages", :target => @material %>
<%= form_for(:material, :url => materials_path) do |f| %>

  <p>
    <%= render(:partial => 'form', :locals => {:f => f}) %>
  </p>
    
  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', materials_path %>